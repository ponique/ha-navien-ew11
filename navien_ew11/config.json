{
  "name": "Navien Wallpad (EW11 TCP)",
  "version": "4.0.0",
  "slug": "navien_ew11",
  "description": "Navien Wallpad Controller via EW11 Direct TCP Connection",
  "url": "https://github.com/ponique/ha-navien-ew11",
  "startup": "application",
  "arch": ["aarch64", "amd64", "armv7"],
  "boot": "auto",
  "build_from": {
    "aarch64": "ghcr.io/hassio-addons/base:14.1.0",
